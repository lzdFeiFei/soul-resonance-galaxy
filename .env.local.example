# 大模型API配置示例
# 复制此文件为 .env.local 并填入你的API密钥

# OpenAI配置
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_OPENAI_MODEL=gpt-4-turbo-preview
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic Claude配置 
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_ANTHROPIC_MODEL=claude-3-opus-20240229
VITE_ANTHROPIC_BASE_URL=https://api.anthropic.com

# Google Gemini配置
VITE_GOOGLE_API_KEY=your_google_api_key  
VITE_GOOGLE_MODEL=gemini-pro
VITE_GOOGLE_BASE_URL=https://generativelanguage.googleapis.com

# 通义千问配置
VITE_QWEN_API_KEY=your_qwen_api_key
VITE_QWEN_MODEL=qwen-max
VITE_QWEN_BASE_URL=https://dashscope.aliyuncs.com

# 智谱AI配置
VITE_ZHIPU_API_KEY=your_zhipu_api_key
VITE_ZHIPU_MODEL=glm-4
VITE_ZHIPU_BASE_URL=https://open.bigmodel.cn

# 百度文心一言配置
VITE_BAIDU_API_KEY=your_baidu_api_key
VITE_BAIDU_SECRET_KEY=your_baidu_secret_key
VITE_BAIDU_MODEL=ERNIE-Bot-4

# 魔搭社区配置（阿里云DashScope）
VITE_MODELSCOPE_API_KEY=your_modelscope_api_key
VITE_MODELSCOPE_MODEL=qwen-max
VITE_MODELSCOPE_BASE_URL=https://dashscope.aliyuncs.com

# 默认使用的模型提供商
# 可选: openai, anthropic, google, qwen, zhipu, baidu, modelscope
VITE_DEFAULT_PROVIDER=openai

# API请求超时时间(毫秒)
VITE_API_TIMEOUT=30000

# 是否开启调试模式
VITE_DEBUG_MODE=false